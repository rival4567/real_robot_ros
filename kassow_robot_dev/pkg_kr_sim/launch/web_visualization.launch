  <launch>

    <!-- This simple launch should be kept to serve as backward compatibility. -->
    <arg name="gripper" default="schunk"/>
    <arg name="topic_ns" value="/rviz_moveit_motion_planning_display/robot_interaction_interactive_marker_topic"/>
    <arg name="fixed_frame" default="world" />
    <arg name="is_sim" default="true" />
    <arg name="roswww_port" default="8000" />
    <arg name="use_roswww" default="false" />
    <arg name="fk_link_name" default="tool_io" />

    <include if="$(eval arg('gripper') == 'schunk')" file="$(find pkg_kr_sg_sim)/launch/kr1410_sg_test.launch" />

    <include if="$(eval arg('gripper') == 'robotiq')" file="$(find pkg_kr_sim)/launch/kr1410_test.launch" />

    <include file="$(find rwt_moveit)/launch/run.launch">
        <arg name="fixed_frame" value="$(arg fixed_frame)" />
        <arg name="is_sim" value="$(arg is_sim)" />
        <arg name="roswww_port" value="$(arg roswww_port)" />
        <arg name="use_roswww" value="$(arg use_roswww)" />
    </include>

    <node name="kr1410_display_trajectory_link" pkg="pkg_kr_sim" type="kr1410_display_planned_path.py" args="-fk_link_name tool_io" output="screen"/>
</launch>